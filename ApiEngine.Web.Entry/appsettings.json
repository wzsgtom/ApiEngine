{
  "AllowedHosts": "*",
  "AppInfo": {
    "GlobalAuthorize": true,
    "Cache": {
      "Redis": {
        "Configuration": "server=127.0.0.1:6379;password=;db=1;timeout=3000",
        "Prefix": "ApiEngine"
      }
    },
    "Log": {
      "Request": true,
      "Response": true,
      "IgnoreKeys": [
        "/api/v1/base/dynamic-table-query"
      ]
    }
  },
  "ConnectionConfigs": [
    {
      "ConfigId": "Default",
      "ConnectionString": "Server=localhost;Database=xxx;User ID=xxx;Password=xxx;Encrypt=True;TrustServerCertificate=True;",
      "DbType": "SqlServer"
    }
  ],
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Serilog": {
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithThreadId"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Hangfire.Storage.SQLite": "Warning",
        "Microsoft": "Warning"
      }
    },
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.File",
      "Serilog.Sinks.Seq"
    ],
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff} [{Level:u3}] {SourceContext:l} {RequestId} {RequestPath}{NewLine}{Message:lj}{NewLine}{Exception}"
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "logs/.log",
          "rollingInterval": "Day",
          "retainedFileCountLimit": 3,
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff} [{Level:u3}] {SourceContext:l} {RequestId} {RequestPath}{NewLine}{Message:lj}{NewLine}{Exception}"
        }
      },
      {
        "Name": "Seq",
        "Args": {
          "apiKey": "tlrhWd9AKw6J0pCdaENN",
          "eventBodyLimitBytes": 10485760,
          "serverUrl": "http://192.168.1.100:5341"
        }
      }
    ]
  },
  "Urls": "http://*:8060"
}